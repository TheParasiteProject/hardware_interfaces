<compatibility-matrix version="1.0" type="framework" level="6">
    <!--
    TODO(b/410953636) leaving the android.hardware.radio entries here for now
    in order to be able to submit this CL before the in-tree devices are fixed.
    These interfaces will be removed ASAP, well before Android 17 release.
    -->
    <hal format="hidl">
        <name>android.hardware.radio</name>
        <version>1.6</version>
        <interface>
            <name>IRadio</name>
            <instance>slot1</instance>
            <instance>slot2</instance>
            <instance>slot3</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>android.hardware.radio</name>
        <version>1.2</version>
        <interface>
            <name>ISap</name>
            <instance>slot1</instance>
            <instance>slot2</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>android.hardware.radio.config</name>
        <!--
        See compatibility_matrix.4.xml on versioning of radio config HAL.
        -->
        <version>1.1</version>
        <interface>
            <name>IRadioConfig</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>android.hardware.radio.config</name>
        <version>1.3</version>
        <interface>
            <name>IRadioConfig</name>
            <instance>default</instance>
        </interface>
    </hal>
</compatibility-matrix>
